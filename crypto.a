

.VEC_ENCRYPT
!byte <chacha20Encrypt, >chacha20Encrypt
.VEC_DECRYPT
!byte <chacha20Decrypt, >chacha20Decrypt
.VEC_SET_KEY
!byte <chacha20SetKey, >chacha20SetKey
.VEC_GET_ID
!byte <chacha20GetId, >chacha20GetId


cryptoInit
    rts


MSG_IV_LEN = 5
MAX_DAY_KEY_LEN = 24

MSG_IV
!byte MSG_IV_LEN
!skip MSG_IV_LEN

DAY_KEY
!byte MAX_DAY_KEY_LEN
!skip MAX_DAY_KEY_LEN


cryptoEncrypt
    jmp (.VEC_ENCRYPT)


cryptoDecrypt
    jmp (.VEC_DECRYPT)


cryptoSetKey
    jmp (.VEC_SET_KEY)


cryptoGetId
    jmp (.VEC_GET_ID)


chacha20Encrypt
    rts


chacha20Decrypt
    rts


chacha20SetKey
    rts


chacha20GetId
    rts
