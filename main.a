!to "cbmcrypt", cbm

CHAROUT = $FFD2

!source "platform.a"

entryPoint
    jmp main

!ifdef C64 {
!source "c64.a"
}

!ifdef VIC20 {
!source "vic20.a"
}

!ifdef PLUS4 {
!source "plus4.a"
}

!ifdef C128 {
!source "c128.a"
}


main
    ldx #0
loop
    txa
    pha
    lda HELLO,x
    jsr CHAROUT
    pla
    tax
    inx
    cpx #HELLO_LEN
    bne loop
    rts