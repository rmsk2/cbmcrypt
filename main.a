!to "cbmcrypt", cbm

CHAROUT = $FFD2

!ifdef C64 {
!source "c64.a"
}

!ifdef VIC20 {
!source "vic20.a"
}

!ifdef PLUS4 {
!source "plus4.a"
}

!ifdef C128 {
!source "c128.a"
}


*=START_ADDR

jmp main

HELLO
!tx "HELLO WORLD"
!byte $0d,$0a

main
    ldx #0
loop
    txa
    pha
    lda HELLO,x
    jsr CHAROUT
    pla
    tax
    inx
    cpx #13
    bne loop
    rts